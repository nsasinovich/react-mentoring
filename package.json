{
  "name": "react-mentoring",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack-dev-server --mode development",
    "build-prod": "webpack --mode production",
    "test": "jest",
    "test:e2e": "nightwatch --env default",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsasinovich/react-mentoring.git"
  },
  "author": "Natallia Sasinovich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsasinovich/react-mentoring/issues"
  },
  "homepage": "https://github.com/nsasinovich/react-mentoring#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^2.38.3",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^6.0.0",
    "nightwatch": "^0.9.21",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "modulePaths": [
      "<rootDir>/"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "setupFiles": [
      "<rootDir>/test/setupTests.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "<rootDir>/test/styleMock.js",
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    }
  }
}
